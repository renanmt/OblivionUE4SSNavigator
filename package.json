{
	"name": "oblivion-ue4ss-navigator",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"copy-data": "bun scripts/copy-data.ts",
		"extract-types": "bun scripts/type-extractor.ts",
		"pages:dev": "wrangler pages dev --compatibility-date=2024-11-12",
		"pages:build": "npm run build && wrangler pages dev .svelte-kit/cloudflare --compatibility-date=2024-11-12",
		"pages:deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-cloudflare": "^4.1.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@types/d3-dsv": "^3.0.7",
		"@types/node": "^22.15.19",
		"autoprefixer": "^10.4.17",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"postcss": "^8.4.35",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.50.0",
		"@toast-ui/editor": "^3.2.2",
		"d3-dsv": "^3.0.1",
		"highlight.js": "^11.11.1",
		"marked": "^15.0.12"
	}
}
